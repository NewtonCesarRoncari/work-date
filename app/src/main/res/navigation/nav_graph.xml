<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/listScheduleFragment">

    <fragment
        android:id="@+id/listClientFragment"
        android:name="com.br.workdate.view.fragment.ListClientFragment"
        android:label="Client List"
        tools:layout="@layout/fragment_list_client" />
    <fragment
        android:id="@+id/listServiceFragment"
        android:name="com.br.workdate.view.fragment.ListServiceFragment"
        android:label="Service List"
        tools:layout="@layout/fragment_list_service" />
    <fragment
        android:id="@+id/listScheduleFragment"
        android:name="com.br.workdate.view.fragment.ListScheduleFragment"
        android:label="Schedule List"
        tools:layout="@layout/fragment_list_schedule">
        <action
            android:id="@+id/action_listScheduleFragment_to_formScheduleFragment"
            app:destination="@id/formScheduleFragment" />
        <action
            android:id="@+id/action_listScheduleFragment_to_searchListClientFragment"
            app:destination="@id/searchListClientFragment" />
    </fragment>
    <fragment
        android:id="@+id/formScheduleFragment"
        android:name="com.br.workdate.view.fragment.FormScheduleFragment"
        android:label="Schedule"
        tools:layout="@layout/fragment_form_schedule">
        <argument
            android:name="client"
            android:defaultValue="@null"
            app:argType="com.br.workdate.model.Client"
            app:nullable="true" />
        <action
            android:id="@+id/action_formScheduleFragment_to_searchListServiceFragment"
            app:destination="@id/searchListServiceFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="service"
            android:defaultValue="@null"
            app:argType="com.br.workdate.model.Service"
            app:nullable="true" />
        <argument
            android:name="schedule"
            android:defaultValue="@null"
            app:argType="com.br.workdate.model.Schedule"
            app:nullable="true" />
        <action
            android:id="@+id/action_formScheduleFragment_to_lovOneFragment"
            app:destination="@id/lovOneFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchListClientFragment"
        android:name="com.br.workdate.view.fragment.SearchListClientFragment"
        android:label="Choose Client"
        tools:layout="@layout/fragment_list_client">
        <action
            android:id="@+id/action_searchListClientFragment_to_searchListServiceFragment"
            app:destination="@id/searchListServiceFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchListServiceFragment"
        android:name="com.br.workdate.view.fragment.SearchListServiceFragment"
        android:label="Choose Service"
        tools:layout="@layout/fragment_list_service">
        <action
            android:id="@+id/action_searchListServiceFragment_to_formScheduleFragment"
            app:destination="@id/formScheduleFragment" />
        <argument
            android:name="schedule"
            android:defaultValue="@null"
            app:argType="com.br.workdate.model.Schedule"
            app:nullable="true" />
        <argument
            android:name="client"
            app:argType="com.br.workdate.model.Client"
            android:defaultValue="@null"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/listReleaseFragment"
        android:name="com.br.workdate.view.fragment.ListReleaseFragment"
        android:label="@string/releases"
        tools:layout="@layout/fragment_list_release" />
    <fragment
        android:id="@+id/lovOneFragment"
        android:name="com.br.workdate.view.fragment.LovOneFragment"
        android:label="Mandinha"
        tools:layout="@layout/lov_screen_one">
        <action
            android:id="@+id/action_lovOneFragment_to_lovTwoFragment"
            app:destination="@id/lovTwoFragment" />
    </fragment>
    <fragment
        android:id="@+id/lovTwoFragment"
        android:name="com.br.workdate.view.fragment.LovTwoFragment"
        android:label="E por falar em voce..."
        tools:layout="@layout/lov_screen_two">
        <action
            android:id="@+id/action_lovTwoFragment_to_lovThreeFragment"
            app:destination="@id/lovThreeFragment" />
    </fragment>
    <fragment
        android:id="@+id/lovThreeFragment"
        android:name="com.br.workdate.view.fragment.LovThreeFragment"
        android:label="Continuando em voce"
        tools:layout="@layout/lov_screen_three">
        <action
            android:id="@+id/action_lovThreeFragment_to_lovFourFragment"
            app:destination="@id/lovFourFragment" />
    </fragment>
    <fragment
        android:id="@+id/lovFourFragment"
        android:name="com.br.workdate.view.fragment.LovFourFragment"
        android:label="Nossos gostos"
        tools:layout="@layout/lov_screen_four">
        <action
            android:id="@+id/action_lovFourFragment_to_lovFiveFragment"
            app:destination="@id/lovFiveFragment" />
    </fragment>
    <fragment
        android:id="@+id/lovFiveFragment"
        android:name="com.br.workdate.view.fragment.LovFiveFragment"
        android:label="Pode acreditar"
        tools:layout="@layout/lov_screen_five">
        <action
            android:id="@+id/action_lovFiveFragment_to_lovSixFragment"
            app:destination="@id/lovSixFragment" />
    </fragment>
    <fragment
        android:id="@+id/lovSixFragment"
        android:name="com.br.workdate.view.fragment.LovSixFragment"
        android:label="E entao..."
        tools:layout="@layout/lov_screen_six">
        <action
            android:id="@+id/action_lovSixFragment_to_lovFragment"
            app:destination="@id/lovFragment" />
    </fragment>
    <fragment
        android:id="@+id/lovFragment"
        android:name="com.br.workdate.view.fragment.LovFragment"
        android:label="..."
        tools:layout="@layout/lov_screen" />
</navigation>